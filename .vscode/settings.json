{
    "python.analysis.typeCheckingMode": "strict",
    "cSpell.words": [
        "aioredis",
        "fastapi",
        "pydantic",
        "rpush",
        "uvicorn"
    ],
    "mypy-type-checker.importStrategy": "fromEnvironment",
    "mypy-type-checker.ignorePatterns": [
        "${workspaceFolder:tests}",
        "${notifications:tests}"
    ]
}